{
  "name": "cpsit/geo-location-service",
  "description": "Get latitude and longitude for an address per google api call",
  "license": "GPL-2.0-or-later",
  "type": "typo3-cms-extension",
  "keywords": [
    "TYPO3",
    "extension"
  ],
  "authors": [
    {
      "name": "Erik Rauchstein",
      "role": "Developer"
    },
    {
      "name": "Dirk Wenzel",
      "role": "Developer"
    },
    {
      "name": "Elias Häußler",
      "email": "e.haeussler@familie-redlich.de",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/CPS-IT/geo_location_service",
  "require": {
    "php": "^7.4 || 8.0.*",
    "ext-json": "*",
    "typo3/cms-core": "^10.4 || ^11.5"
  },
  "require-dev": {
    "typo3/coding-standards": "^0.5.0"
  },
  "replace": {
    "geo_location_service": "self.version",
    "typo3-ter/geo-location-service": "self.version"
  },
  "autoload": {
    "psr-4": {
      "CPSIT\\GeoLocationService\\": "Classes/"
    }
  },
  "config": {
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    },
    "sort-packages": true
  }
}
